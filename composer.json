{
    "name": "pkpudev/yii2-app-advanced",
    "description": "Yii 2 Advanced Dev Template",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "~2.0.13.3",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-shell": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.1",
        "dmstr/yii2-adminlte-asset": "~2.4.0",
        "johnitvn/yii2-ajaxcrud": "*",
        "kartik-v/yii2-builder": "*",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-tabs-x": "*",
        "kartik-v/yii2-widgets": "*",
        "mdmsoft/yii2-admin": "~2.0",
        "notamedia/yii2-sentry": "^1.2",
        "pkpudev/yii2-components": "*",
        "pkpudev/yii2-simplesaml": "*",
        "vlucas/phpdotenv": "2.*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "yiisoft/yii2-gii": "~2.0",
        "yiisoft/yii2-faker": "~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    }
}
